                android:layout_height="wrap_content" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/slog_defined_aplog"
                    android:textSize="10sp" />
            </LinearLayout>

            <LinearLayout
